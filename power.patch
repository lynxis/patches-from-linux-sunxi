diff --git a/drivers/power/axp_power/axp18-mfd.h b/drivers/power/axp_power/axp18-mfd.h
new file mode 100644
index 0000000..ecfdc3f
--- /dev/null
+++ b/drivers/power/axp_power/axp18-mfd.h
@@ -0,0 +1,241 @@
+/*
+ * drivers/power/axp_power/axp18-mfd.h
+ *
