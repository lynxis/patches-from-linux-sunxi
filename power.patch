+int ADC_Freq_Get(struct axp_charger *charger)
+{
+	uint8_t  temp;
+	int  rValue = 25;
+
+	axp_read(charger->master, AXP20_ADC_CONTROL3,&temp);
+	temp &= 0xc0;
+	switch(temp >> 6)
