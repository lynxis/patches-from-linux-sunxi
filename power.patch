+	AXP19_LDO( 2,     1800,     3300, 100,ANALOG1,    4,    4,  LDO2EN,    2),//ldo2 for analog1 
+	AXP19_LDO( 3,      700,     3500,  25,DIGITAL,    0,    7,  LDO3EN,    2),//ldo3 for digital
+	AXP19_LDO( 4,     1800,     3300, 100,ANALOG2,    0,    4,  LDO4EN,    3),//ldo4 for analog2
