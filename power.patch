+	AXP18_BUCK(	3,		DCDC3MIN,	DCDC3MAX,	100, 	MOMERY,	2,	3,	DCDC3EN,	4),//dcdc3 for momery
+	AXP18_SW(  	1,		2800,			3500,			100,	IO,			4,	3,	SW1EN,		7),//sw1 for sdram
+	AXP18_SW(  	2,		2800,			3500, 		100,	IO,			4,	3,	SW2EN,		6),//sw2 for sd card
+};
